{
  "name": "urbex",
  "version": "1.0.0",
  "description": "A lightweight HTTP client with built in cache support and more",
  "main": "dist/cjs/urbex.min.js",
  "module": "dist/esm/urbex.min.js",
  "types": "dist/@types/urbex.d.ts",
  "files": [
    "dist",
    "lib",
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "test": "npm run test:mocha && npm run test:karma",
    "test:mocha": "mocha --require ts-node/register --recursive test/**/*.test.ts",
    "test:karma": "karma start karma.config.ts --single-run",
    "lint:check": "eslint lib --ignore-path .gitignore --ext .ts",
    "lint:fix": "eslint lib --ignore-path .gitignore --ext .ts --fix",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "build": "rimraf dist && tsc && rollup -c -m rollup.config.js --bundleConfigAsCjs",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:dry-run": "standard-version --dry-run",
    "publish:tag": "git push origin --follow-tags",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orison-networks/urbex.git"
  },
  "bugs": {
    "url": "https://github.com/orison-networks/urbex/issues"
  },
  "homepage": "https://github.com/orison-networks/urbex#readme",
  "keywords": [
    "http",
    "caching",
    "network",
    "nodejs",
    "javascript",
    "xhr",
    "promise"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/chai": "^4.3.3",
    "@types/karma": "^6.3.3",
    "@types/mocha": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "chai": "^4.3.6",
    "husky": "^8.0.0",
    "karma": "^6.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "^5.1.0",
    "karma-mocha": "^2.0.1",
    "karma-typescript": "^5.5.3",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.2.3",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-dts": "^5.0.0",
    "standard-version": "^9.5.0",
    "terser": "^5.15.1",
    "ts-loader": "^9.4.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0"
  },
  "standard-version": {
    "scripts": {
      "posttag": "npm run publish:tag"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ]
  },
  "browser": {
    "./lib/core/api/http.ts": "./lib/core/api/xhr.ts"
  },
  "author": "Orison Networks",
  "contributors": [
    "Michael Cizek <itsmichaelbtw@gmail.com>"
  ],
  "license": "MIT"
}
